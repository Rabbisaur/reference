@Article{Fetsch2014,
  Title                    = {Effects of cortical microstimulation on confidence in a perceptual decision.},
  Author                   = {Fetsch, Christopher R. and Kiani, Roozbeh and Newsome, William T. and Shadlen, Michael N.},
  Journal                  = {Neuron},
  Year                     = {2014},

  Month                    = {Aug},
  Number                   = {4},
  Pages                    = {797--804},
  Volume                   = {83},

  Abstract                 = {Decisions are often associated with a degree of certainty, or confidence--an estimate of the probability that the chosen option will be correct. Recent neurophysiological results suggest that the central processing of evidence leading to a perceptual decision also establishes a level of confidence. Here we provide a causal test of this hypothesis by electrically stimulating areas of the visual cortex involved in motion perception. Monkeys discriminated the direction of motion in a noisy display and were sometimes allowed to opt out of the direction choice if their confidence was low. Microstimulation did not reduce overall confidence in the decision but instead altered confidence in a manner that mimicked a change in visual motion, plus a small increase in sensory noise. The results suggest that the same sensory neural signals support choice, reaction time, and confidence in a decision and that artificial manipulation of these signals preserves the quantitative relationship between accumulated evidence and confidence.},
  Doi                      = {10.1016/j.neuron.2014.07.011},
  File                     = {:Fetsch2014.pdf:PDF},
  Institution              = {Howard Hughes Medical Institute, Department of Neuroscience and Kavli Institute for Brain Science, Columbia University, New York, NYï¿½0ï¿½0ï¿½0ï¿½210032, USA. Electronic address: shadlen@columbia.edu.},
  Language                 = {eng},
