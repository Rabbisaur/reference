@Article{Martinez1999,
  Title                    = {Involvement of striate and extrastriate visual cortical areas in spatial attention.},
  Author                   = {A. Martï¿½0ï¿½1ï¿½0ï¿½2nez and L. Anllo-Vento and M. I. Sereno and L. R. Frank and R. B. Buxton and D. J. Dubowitz and E. C. Wong and H. Hinrichs and H. J. Heinze and S. A. Hillyard},
  Journal                  = {Nat Neurosci},
