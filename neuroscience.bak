@Article{Lee2007,
  Title                    = {Spatial attention and the latency of neuronal responses in macaque area V4.},
  Author                   = {Lee, Joonyeol and Williford, Tori and Maunsell, John H R.},
  Journal                  = {J Neurosci},
  Year                     = {2007},

  Month                    = {Sep},
