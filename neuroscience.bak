@Article{Wolf2009,
  Title                    = {The effects of implicit attentional learning and habituation on inhibition of return.},
  Author                   = {Kerstin Wolf and Daniel Ebeling and Notger G Mï¿½0ï¿½1ï¿½0ï¿½4ller},
  Journal                  = {Atten Percept Psychophys},
