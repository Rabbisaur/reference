}

@Article{Lee1998,
  Title                    = {The role of the primary visual cortex in higher level vision.},
  Author                   = {Lee, T. S. and Mumford, D. and Romero, R. and Lamme, V. A.},
  Journal                  = {Vision Res},
  Year                     = {1998},

  Month                    = {Aug},
  Number                   = {15-16},
  Pages                    = {2429--2454},
  Volume                   = {38},

  Abstract                 = {In the classical feed-forward, modular view of visual processing, the primary visual cortex (area V1) is a module that serves to extract local features such as edges and bars. Representation and recognition of objects are thought to be functions of higher extrastriate cortical areas. This paper presents neurophysiological data that show the later part of V1 neurons' responses reflecting higher order perceptual computations related to Ullman's (Cognition 1984; 18:97-159) visual routines and Marr's (Vision NJ: Freeman 1982) full primal sketch, 2 1/2D sketch and 3D model. Based on theoretical reasoning and the experimental evidence, we propose a possible reinterpretation of the functional role of V1. In this framework, because of V1 neurons' precise encoding of orientation and spatial information, higher level perceptual computations and representations that involve high resolution details, fine geometry and spatial precision would necessarily involve V1 and be reflected in the later part of its neurons' activities.},
  File                     = {:pdf/Lee1998_v1_responseto_medialaxis.pdf:PDF},
  Institution              = {Center for the Neural Basis of Cognition, Carnegie Mellon University, Pittsburgh, PA 15213, USA. tai@cnbc.cmu.edu},
  Keywords                 = {Action Potentials; Animals; Brain Mapping; Cues; Fixation, Ocular; Form Perception, physiology; Fovea Centralis, physiology; Macaca mulatta; Models, Neurological; Saccades; Visual Cortex, physiology;v1v2contour},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {wangfeng},
  Pii                      = {S0042-6989(97)00464-1},
  Pmid                     = {9798008},
  Review                   = {06May2016

The main finding in this paper is ~30% v1 cell respond to the medial axis of a figure. It also try to put the findings of the extra RF modulation into a revised unified theory of visual cortex which emphansis on recurrent and feedback connections.},
  Timestamp                = {2016.05.06}
}

@Article{Lee2001,
  Title                    = {Dynamics of subjective contour formation in the early visual cortex.},
  Author                   = {T. S. Lee and M. Nguyen},
  Journal                  = {Proc Natl Acad Sci U S A},
  Year                     = {2001},

  Month                    = {Feb},
  Number                   = {4},
  Pages                    = {1907--1911},
  Volume                   = {98},

  Abstract                 = {To elucidate the roles of visual areas V1 and V2 and their interaction in early perceptual processing, we studied the responses of V1 and V2 neurons to statically displayed Kanizsa figures. We found evidence that V1 neurons respond to illusory contours of the Kanizsa figures. The illusory contour signals in V1 are weaker than in V2, but are significant, particularly in the superficial layers. The population averaged response to illusory contours emerged 100 msec after stimulus onset in the superficial layers of V1, and around 120--190 msec in the deep layers. The illusory contour response in V2 began earlier, occurring at 70 msec in the superficial layers and at 95 msec in the deep layers. The temporal sequence of the events suggests that the computation of illusory contours involves intercortical interaction, and that early perceptual organization is likely to be an interactive process.},
  Doi                      = {10.1073/pnas.031579998},
  File                     = {Lee2001.pdf:Lee2001.pdf:PDF},
  Institution              = {Center for the Neural Basis of Cognition, Carnegie Mellon University, Pittsburgh, PA 15213, USA. tai@cs.cmu.edu},
  Keywords                 = {Animals; Form Perception, physiology; Macaca mulatta; Neurons, physiology; Photic Stimulation; Visual Cortex, cytology/physiology},
  Language                 = {eng},
  Medline-pst              = {ppublish},
